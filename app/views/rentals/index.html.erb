<h1>Locações</h1>

<%= link_to 'Registrar locação', new_rental_path %>

<% @rentals.each do |rental| %>
  <div>
    <p>
      <strong>Data inicial:</strong> <%= l rental.start_date %>
    </p>
    <p>
      <strong>Data final:</strong> <%= l rental.end_date %>
    </p>
    <p>
      <strong>Cliente:</strong> <%= rental.client.identification %>
    </p>
    <p>
      <strong>Categoria:</strong> <%= rental.car_category.name %>
    </p>
  </div>
<% end %>